{
  "version": 2,
  "rewrites": [
    {
      "source": "/health",
      "destination": "/api/health"
    },
    {
      "source": "/admin/:path*",
      "destination": "/api/index"
    },
    {
      "source": "/api/(?!health)",
      "destination": "/api/index"
    },
    {
      "source": "/",
      "destination": "/index.html"
    }
  ]
}
